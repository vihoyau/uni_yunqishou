@charset "UTF-8";
/**
 * 下方引入的为uView UI的集成样式文件，为scss预处理器，其中包含了一些"u-"开头的自定义变量
 * 使用的时候，请将下面的一行复制到您的uniapp项目根目录的uni.scss中即可
 * uView自定义的css类名和scss变量，均以"u-"开头，不会造成冲突，请放心使用 
 */
.log-list {
  min-height: 213rpx;
  background: #ffffff;
  margin-bottom: 10rpx;
  padding-bottom: 10rpx;
}
.log-list .head {
  padding: 0 35rpx;
  height: 80rpx;
  border-bottom: 1rpx solid #eee;
  margin-bottom: 20rpx;
}
.log-list .head .title {
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
}
.log-list .head .num {
  font-size: 28rpx;
  font-weight: 500;
  color: #7063d2;
}
.log-list .item {
  padding: 0 30rpx 10rpx;
}
.log-list .item .item-icon {
  color: #c0c0c0;
  font-size: 36rpx;
  margin-right: 8rpx;
}
.log-list .item .item-title {
  width: 180rpx;
  font-size: 24rpx;
  font-weight: 400;
  color: #c0c0c0;
}
.log-list .item .status-text {
  font-size: 24rpx;
  font-weight: 500;
  color: #05c3a1;
}
.log-list .item .time {
  font-size: 24rpx;
  font-weight: 400;
  color: #c0c0c0;
}

