.state-item {
  height: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  background-color: #fff;
  border-bottom: 1rpx solid rgba(153, 153, 153, 0.5);
}
.state-item .state-title {
  color: #666;
  font-weight: 500;
  font-size: 28rpx;
  line-height: 90rpx;
}
.state-item .title-active {
  color: #333;
}
.state-item .underline {
  display: block;
  width: 68rpx;
  height: 4rpx;
  background: #fff;
  border-radius: 2rpx;
}
.state-item .underline-active {
  background: #5e49c3;
  display: block;
  width: 68rpx;
  height: 4rpx;
  border-radius: 2rpx;
}
.wallet-wrap {
  background-color: #fff;
  padding: 30rpx;
}
.wallet-card {
  width: 690rpx;
  height: 301rpx;
  background: url("http://file.shopro.top/imgs/commission/commission_card_bg.png") no-repeat;
  background-size: 100% 100%;
  border-radius: 20rpx;
  padding: 30rpx;
  position: relative;
  box-shadow: 1rpx 5rpx 16rpx 0px rgba(94, 73, 195, 0.81);
}
.wallet-card .head-box {
  margin-bottom: 20rpx;
}
.wallet-card .head-box .head-title {
  font-size: 24rpx;
  font-weight: 400;
  color: #ffffff;
}
.wallet-card .head-box .look-btn {
  color: #fff;
  font-size: 30rpx;
  background: none;
  padding: 0;
  margin-left: 20rpx;
}
.wallet-card .head-box .look-btn .cuIcon-attentionfill,
.wallet-card .head-box .look-btn .cuIcon-attentionforbidfill {
  line-height: 30rpx;
}
.wallet-card .head-box .look-btn .cuIcon-attentionfill {
  line-height: 30rpx;
}
.wallet-card .card-num {
  font-size: 40rpx;
  font-weight: 500;
  color: #fefefe;
  margin-bottom: 30rpx;
}
.wallet-card .card-bottom .card-item .item-title {
  font-size: 24rpx;
  font-weight: 400;
  color: #ffffff;
  margin-bottom: 10rpx;
}
.wallet-card .card-bottom .card-item .item-value {
  font-size: 30rpx;
  font-weight: 500;
  color: #fefefe;
}
.wallet-card .draw-btn {
  position: absolute;
  top: 35rpx;
  right: 35rpx;
  width: 121rpx;
  line-height: 58rpx;
  background: #ffffff;
  border-radius: 29rpx;
  padding: 0;
  font-size: 24rpx;
  font-weight: 500;
  color: #5848c4;
}
.head_box {
  height: 120rpx;
  padding: 0 30rpx;
  background-color: #f6f6f6;
}
.head_box .date-btn {
  background-color: #fff;
  line-height: 54rpx;
  border-radius: 27rpx;
  padding: 0 20rpx;
  font-size: 24rpx;
  font-weight: 500;
  color: #666666;
}
.head_box .total-box {
  font-size: 24rpx;
  font-weight: 500;
  color: #666666;
}
.item-box {
  margin: 20rpx 0;
}
.log-item {
  height: 142rpx;
  background-color: #fff;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #eee;
}
.log-item .item-left .log-img {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-right: 24rpx;
}
.log-item .item-left .log-name {
  font-size: 28rpx;
  font-weight: 500;
  color: #333333;
  margin-bottom: 12rpx;
}
.log-item .item-left .log-notice {
  font-size: 22rpx;
  font-weight: 500;
  color: #c0c0c0;
}
.log-item .item-right .log-num {
  font-size: 28rpx;
  font-weight: 500;
  color: #05c3a1;
}
.log-item .item-right .log-date {
  font-size: 24rpx;
  font-weight: 400;
  color: #c0c0c0;
}

