.sh-title-card {
  width: 750rpx;
}
.title-box {
  width: 710rpx;
  height: 88rpx;
  margin: 0 auto;
  position: relative;
  border-radius: 30rpx;
}
.title-box .title-bg {
  width: 100%;
  height: 100%;
}
.title-box .title-text {
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  font-weight: bold;
}

