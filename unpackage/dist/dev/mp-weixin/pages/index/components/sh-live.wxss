.live-el {
  background: #fff;
  padding: 30rpx 20rpx;
}
.live-el .head {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.live-el .head-title {
  font-size: 32rpx;
  font-weight: bold;
  color: #333333;
}
.live-el .head-more .more-text {
  font-size: 22rpx;
  font-weight: 500;
  color: #333333;
}
.live-el .head-more .more-icon {
  font-size: 24rpx;
  color: #333333;
}
.live-el .content-two {
  width: 710rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.live-el .content-two__item {
  margin-right: 20rpx;
  margin-top: 20rpx;
  width: 345rpx;
}
.live-el .content-two__item:nth-child(2n) {
  margin-right: 0;
}

