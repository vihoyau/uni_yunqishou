.tip-list-box {
  background-color: #f6f6f6;
  padding: 20rpx;
}
.tip-list {
  font-size: 28rpx;
  background-color: #fff;
  border-radius: 20rpx;
}
.tip-list .tag-box {
  border: 1px solid #ff0000;
  display: inline-block;
  font-size: 20rpx;
  line-height: 30rpx;
  padding: 0 10rpx;
  color: #ff0000;
  border-radius: 8rpx;
}

